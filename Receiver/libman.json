{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-growl@1.0.0",
      "destination": "wwwroot/lib/bootstrap-growl/"
    },
    {
      "library": "notify.js@0.0.3",
      "destination": "wwwroot/notify.js/",
      "files": [
        "notify.bundle.js"
      ]
    }
  ]
}